# Backend API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=10000

# App Configuration
VITE_APP_NAME="Ryder Cup Manager"
VITE_APP_VERSION=1.0.0

# Sentry Configuration
# DSN - Data Source Name (obtenerlo de Sentry.io > Settings > Projects > Client Keys)
VITE_SENTRY_DSN=your-sentry-dsn-here

# Environment - Entorno actual (development, staging, production)
VITE_SENTRY_ENVIRONMENT=development

# Debug - Habilitar logs de debug de Sentry (solo desarrollo)
VITE_SENTRY_DEBUG=true

# Traces Sample Rate - Porcentaje de transacciones a capturar (0.0 - 1.0)
# development: 1.0 (100%), production: 0.1 (10%)
VITE_SENTRY_TRACES_SAMPLE_RATE=1.0

# Profiles Sample Rate - Porcentaje de perfiles de rendimiento (0.0 - 1.0)
# development: 1.0 (100%), production: 0.1 (10%)
VITE_SENTRY_PROFILES_SAMPLE_RATE=1.0

# Replays Session Sample Rate - Porcentaje de sesiones normales a grabar (0.0 - 1.0)
# development: 0.1 (10%), production: 0.05 (5%)
VITE_SENTRY_REPLAYS_SESSION_SAMPLE_RATE=0.1

# Replays On Error Sample Rate - Porcentaje de sesiones con error a grabar (0.0 - 1.0)
# development: 1.0 (100%), production: 1.0 (100%)
VITE_SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1.0

# Auto Session Tracking - Seguimiento autom√°tico de sesiones
VITE_SENTRY_AUTO_SESSION_TRACKING=true

# Attach Stack Trace - Adjuntar stack trace a mensajes (no solo errores)
VITE_SENTRY_ATTACH_STACKTRACE=true

# Enable Feedback Widget - Habilitar widget de feedback para usuarios
VITE_SENTRY_ENABLE_FEEDBACK=false

# Feature Flags (optional)
VITE_ENABLE_DARK_MODE=false
VITE_ENABLE_I18N=false
